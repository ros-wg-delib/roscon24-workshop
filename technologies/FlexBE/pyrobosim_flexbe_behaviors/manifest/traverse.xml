<?xml version="1.0" encoding="UTF-8"?>

<behavior name="Traverse">

    <executable package_path="pyrobosim_flexbe_behaviors.traverse_sm" class="TraverseSM" />
    <tagstring>pyrobosim, delib_ws</tagstring>
    <author>David Conner</author>
    <date>Sun Sep 08 2024</date>
    <description>
        Traverse the world opening doors as required
    </description>


    <!-- Contained Behaviors -->
    <contains name="Go Beh" />
    <contains name="Through Door" />

    <!-- Available Parameters -->
    <params>

        <param type="text" name="target" default="charger" label="target" hint="Sets the target" />

    </params>


</behavior>
