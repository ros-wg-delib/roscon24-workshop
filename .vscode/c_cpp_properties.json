{
    "configurations": [
      {
        "browse": {
          "databaseFilename": "${default}",
          "limitSymbolsToIncludedHeaders": false
        },
        "includePath": [
          "/opt/ros/jazzy/include/**",
          "/usr/include/**",
          "${workspaceFolder}/.colcon/build/**"
        ],
        "name": "ROS",
        "intelliSenseMode": "gcc-x64",
        "compilerPath": "/usr/bin/gcc",
        "cStandard": "gnu11",
        "cppStandard": "c++17",
        "configurationProvider": "ms-vscode.cmake-tools",
        "compileCommands": "${workspaceFolder}/.colcon/build/compile_commands.json"
      }
    ],
    "version": 4
}
